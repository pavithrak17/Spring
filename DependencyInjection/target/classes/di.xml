<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

 <bean id="car" class="com.tnsif.dependencyinjection.Car">
<constructor-arg value="Honda" ></constructor-arg>
<constructor-arg value="2022"></constructor-arg>
<property name="name" value="Suzuki"></property>
<property name="model" value="2020"></property>
</bean>
